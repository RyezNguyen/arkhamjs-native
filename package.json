{
  "name": "arkhamjs-native",
  "version": "1.5.2",
  "description": "An ES6 Flux library for React Native",
  "license": "MIT",
  "main": "index.js",
  "keywords": [
    "arkhamjs",
    "flux",
    "immutable",
    "nitrogenlabs",
    "react-native"
  ],
  "author": "Giraldo Rosales <giraldo@nitrogenlabs.com> (http://nitrogenlabs.com/)",
  "repository": {
    "type": "git",
    "url": "http://github.com/nitrogenlabs/arkhamjs-native"
  },
  "homepage": "https://arkhamjs.com",
  "bugs": {
    "url": "https://github.com/nitrogenlabs/arkhamjs-native/issues"
  },
  "engines": {
    "node": ">6.0.0",
    "npm": ">3.3.6"
  },
  "scripts": {
    "docs": "esdoc -c esdoc.json",
    "test": "jest",
    "prepublish": "NODE_ENV=production node_modules/babel-cli/bin/babel.js src --out-dir lib"
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "events": "^1.1.1",
    "immutable": "^3.8.1",
    "prop-types": "^15.5.10",
    "react": "16.0.0-alpha.12",
    "react-native": "0.45.1",
    "react-router-native": "^4.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-plugin-remove-comments": "^2.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-babili": "0.1.3",
    "babel-preset-env": "^1.5.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-node6": "^11.0.0",
    "babel-preset-react-native": "^2.0.0",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "babel-preset-stage-0": "^6.24.1",
    "esdoc": "^0.5.2",
    "eslint": "^4.0.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-react": "^7.1.0",
    "eslint-plugin-react-native": "^2.3.2",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^20.0.4",
    "jest-haste-map": "^20.0.4",
    "mock-async-storage": "^1.0.3",
    "regenerator-runtime": "^0.10.5"
  },
  "jest": {
    "preset": "react-native",
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(!react-native|!react-router-native)/"
    ],
    "verbose": true,
    "setupFiles": [
      "<rootDir>/test/setup.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
